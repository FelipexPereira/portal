{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Program",
        "program": "${workspaceFolder}/index.js",
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/**/*.js"]
      },
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome against localhost",
        "url": "http://localhost:8080/api/login",
        "webRoot": "${workspaceFolder}"
      }
    ]
}  
